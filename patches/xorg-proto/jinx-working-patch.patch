Subject: [PATCH] Ironclad specific changes

---
 include/X11/Xos.h   | 2 +-
 include/X11/Xos_r.h | 2 +-
 2 files changed, 2 insertions(+), 2 deletions(-)

diff --git a/include/X11/Xos.h b/include/X11/Xos.h
index 28dfc67..841e7ee 100644
--- a/include/X11/Xos.h
+++ b/include/X11/Xos.h
@@ -60,7 +60,7 @@ in this Software without prior written authorization from The Open Group.
  */
 
 # include <string.h>
-# if defined(__SCO__) || defined(__UNIXWARE__) || defined(__sun) || defined(__CYGWIN__) || defined(_AIX) || defined(__APPLE__)
+# if defined(__SCO__) || defined(__UNIXWARE__) || defined(__sun) || defined(__CYGWIN__) || defined(_AIX) || defined(__APPLE__) || defined(__ironclad__)
 #  include <strings.h>
 # else
 #  ifndef index
diff --git a/include/X11/Xos_r.h b/include/X11/Xos_r.h
index f963b64..5ade17b 100644
--- a/include/X11/Xos_r.h
+++ b/include/X11/Xos_r.h
@@ -318,7 +318,7 @@ static __inline__ void _Xpw_copyPasswd(_Xgetpwparams p)
   (_Xos_processUnlock), \
   (p).pwp )
 
-#elif !defined(_POSIX_THREAD_SAFE_FUNCTIONS) && !defined(__APPLE__)
+#elif !defined(_POSIX_THREAD_SAFE_FUNCTIONS) && !defined(__APPLE__) && !defined(__ironclad__)
 # define X_NEEDS_PWPARAMS
 typedef struct {
   struct passwd pws;
